type Query {
    getSummoner(name: String): SummonerResponseDto!
    getRanking(page: Int): SummonerRankingResponseDto
    getMatches(name: String!, page: Int): MatchResponseDto
    getChang: ChangResponseDto
}

type SummonerRankingResponseDto{
    summonerResponseDto: [SummonerResponseDto]
    page: Int
}

type SummonerResponseDto {
    email: String
    code: Int
    name: String
    nickname: String
    role: String
    isGraduate: Boolean
    enrolledAt: Int
    grade: Int
    classNo: Int
    studentNo: Int
    gameName: String!
    tagLine: String!
    profileIcon: Int!
    level: Int!
    soloTier: String
    soloLp: Int
    soloWins: Int
    soloLoses: Int
    flexTier: String
    flexLp: Int
    flexWins: Int
    flexLoses: Int
    mostChampions: [String]
    soloPoint: Int!
    flexPoint: Int!
    ranking: Int!
    userCount: Int
}

type MatchResponseDto{
    matches: [MatchInfoResponseDto]
    page: Int
}

type MatchInfoResponseDto{
    gameType: String
    isWin: Boolean
    gameStartedAt: Int
    gameDuration: Int
    participants: [ParticipantResponseDto]
}

type ParticipantResponseDto{
    gameName: String
    tagLine: String
    soloTier: String
    soloPoint: Int
    flexTier: String
    flexPoint: Int
    level: Int
    champion: Champion
    championLevel: Int
    lane: Int
    team: String
    spell1: Spell
    spell2: Spell
    mainPerk: Perk
    subPerk: Perk
    killRate: Int
    kills: Int
    assists: Int
    deaths: Int
    damage: Int
    gainDamage: Int
    cs: Int
    visionScore: Int
    sightWard: Int
    visionWard: Int
    items: [Item]
    ward: Item
}

type Perk{
    id: Int
    name: String
}

type Champion{
    id: String
    name: String
}

type Item{
    id: Int
    name: String
}

type Spell{
    id: String
    name: String
}

type ChangResponseDto{
    summonerInfo: SummonerResponseDto
    changInfo: ChangInfoDto
}

type ChangInfoDto{
    playedGames: Int
    winGames: Int
    loseGames: Int
    kills: Int
    deaths: Int
    assists: Int
    goldSpent: Int
    skillUse: Int
    playTime: Int
    spellUse: Int
    damage: Int
    gainDamage: Int
    wardPlaced: Int
    visionScore: Int
    cs: Int
}